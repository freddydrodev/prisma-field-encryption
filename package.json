{
  "name": "@freddydrodev/prisma-field-encryption",
  "version": "0.0.0-semantically-released",
  "description": "Transparent field-level encryption at rest for Prisma (Updated for Prisma 6.14.0)",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./generator": {
      "types": "./dist/generator/main.d.ts",
      "import": "./dist/generator/main.mjs",
      "require": "./dist/generator/main.js"
    }
  },
  "license": "MIT",
  "bin": {
    "prisma-field-encryption": "./dist/generator/main.js"
  },
  "author": {
    "name": "Freddy Drodev",
    "email": "freddy@freddydrodev.com",
    "url": "https://freddydro.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/47ng/prisma-field-encryption"
  },
  "keywords": [
    "prisma",
    "middleware",
    "encryption",
    "aes-256-gcm"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf ./dist ./coverage",
    "build": "tsup",
    "dev": "tsup --watch",
    "deploy": "tsup && npm publish --access public",
    "postbuild": "cd node_modules/.bin && ln -sf ../../dist/generator/main.js ./prisma-field-encryption",
    "generate": "run-s generate:*",
    "generate:prisma": "prisma generate",
    "test": "run-s test:**",
    "test:types": "tsc --noEmit",
    "test:unit": "jest --config jest.config.unit.json",
    "test:integration": "jest --config jest.config.integration.json --runInBand",
    "test:coverage:merge": "nyc merge ./coverage ./coverage/coverage-final.json",
    "test:coverage:report": "nyc report -t ./coverage --r html -r lcov -r clover",
    "ci": "run-s build test",
    "premigrate": "run-s build generate",
    "migrate": "ts-node ./src/tests/migrate.ts"
  },
  "dependencies": {
    "@47ng/cloak": "^1.2.0",
    "@47ng/codec": "^1.1.0",
    "debug": "^4.4.1",
    "immer": "^10.1.1",
    "object-path": "^0.11.8",
    "zod": "^4.0.17"
  },
  "peerDependencies": {
    "@prisma/client": ">=4.7.0 <7.0.0"
  },
  "devDependencies": {
    "@prisma/client": "6.14.0",
    "@prisma/generator-helper": "^6.14.0",
    "@prisma/internals": "^6.14.0",
    "@types/debug": "^4.1.12",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/object-path": "^0.11.4",
    "jest": "^30.0.5",
    "npm-run-all": "^4.1.5",
    "nyc": "^17.1.0",
    "prisma": "^6.14.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "jest": {
    "verbose": true,
    "preset": "ts-jest/presets/js-with-ts",
    "testEnvironment": "node"
  },
  "prettier": {
    "arrowParens": "avoid",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}
